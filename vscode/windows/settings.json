{
  "breadcrumbs.enabled": true,
  "breadcrumbs.filePath": "on",
  "editor.autoIndent": "full",
  "editor.cursorSurroundingLines": 5,
  "editor.formatOnSave": true,
  "editor.wordWrap": "off",
  "editor.fontFamily": "'Fira Code', 'Droid Sans Mono', 'monospace', monospace",
  "editor.bracketPairColorization.enabled": true,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.fontLigatures": true,
  "editor.insertSpaces": false,
  "editor.fontSize": 15,
  "editor.suggestSelection": "first",
  "editor.tabSize": 4,
  "editor.cursorStyle": "line",
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.formatOnPaste": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[html]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[toml]": {
    "editor.tabSize": 2
  },
  "[markdown]": {
    "editor.formatOnSave": false
  },
  "[makefile]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": false,
    "editor.detectIndentation": false
  },
  "[json]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.organizeImports": true,
      "source.fixAll": true
    }
  },
  "[svelte]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jinja-html]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[python]": {
    "editor.tabSize": 4,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform",
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
      "source.formatAll.terraform": true
    }
  },
  "[terraform-vars]": {
    "editor.defaultFormatter": "hashicorp.terraform",
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
      "source.formatAll.terraform": true
    }
  },
  "remote.containers.defaultExtensions": [
    "eamodio.gitlens",
    "oderwat.indent-rainbow",
    "esbenp.prettier-vscode",
    "redhat.vscode-yaml",
    "coenraads.bracket-pair-colorizer"
  ],
  "remote.extensionKind": {
    "2gua.rainbow-brackets": ["workspace"],
    "aaron-bond.better-comments": ["workspace"],
    "coenraads.bracket-pair-colorizer": ["workspace"]
  },
  "eslint.codeActionsOnSave.mode": "all",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "auto-close-tag.disableOnLanguage": ["typescript"],
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 5000,
  "files.trimFinalNewlines": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.idea": true,
    "**/.history": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.venv/**": true,
    "**/node_modules/*/**": true,
    "**/.history": true
  },
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "gitlens.keymap": "alternate",
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "python.linting.lintOnSave": true,
  "python.formatting.provider": "black",
  "python.analysis.indexing": true,
  "python.formatting.blackArgs": ["--line-length", "120"],
  "python.autoComplete.extraPaths": ["./src"],
  "python.envFile": "${workspaceFolder}/.env",
  "python.analysis.typeCheckingMode": "basic",
  "terminal.integrated.fontFamily": "'Fira Code', 'PowerlineSymbols'",
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.tabs.enabled": false,
  "python.terminal.activateEnvInCurrentTerminal": true,
  "python.terminal.activateEnvironment": true,
  "window.restoreWindows": "all",
  "window.titleBarStyle": "custom",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Dracula",
  "workbench.startupEditor": "welcomePage",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "python.languageServer": "Pylance",
  "files.associations": {
    "*.tsx": "typescriptreact"
  },
  "python.analysis.extraPaths": ["./src"],
  "python.analysis.completeFunctionParens": true,
  "files.eol": "\n",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "redhat.telemetry.enabled": false,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "svelte.enable-ts-plugin": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.handleKeys": {
    "<C-f>": false
  },
  "vim.easymotionMarkerBackgroundColor": "#7e57c2",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [" "],
      "after": ["leader", "leader", "leader", "b", "d", "w"]
    }
  ],
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.hlsearch": true,
  "vim.leader": "<Space>",
  "vim.smartRelativeLine": true,
  "macros": {
    "showTerminal": [
      "workbench.action.terminal.toggleTerminal",
      "workbench.action.terminal.focus"
    ],
    "hideTerminal": ["workbench.action.terminal.toggleTerminal"]
  },
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "python.analysis.autoSearchPaths": false,
  "window.zoomLevel": -1
}
